ta\"\n      v-bind:key=\"idx\"\n      v-bind:active=\"moleState\"\n      v-bind:moleId=\"idx\"\n      v-on:whack=\"handleWhack\"\n      v-on:miss=\"handleMiss\"\n    >\n    </Mole>\n  </div>\n</template>\n\n<script>\nimport Mole from './Mole';\n\nexport default {\n  name: 'Moles',\n  components: {\n    Mole,\n  },\n  props: ['moleData', 'gameActive'],\n  methods: {\n    handleWhack: function(moleId) {\n      this.$emit('whack', moleId);\n    },\n    handleMiss: function() {\n      this.$emit('miss');\n    },\n  },\n  computed: {\n    classNames: function() {\n      return {\n        'moles-container': true, // 必ず表示する\n        'game-active': this.gameActive, // ゲームが遊ばれている時だけ\n      };\n    }\n  },\n};\n</script>\n\n<style>\n.moles-container {\n  margin-top: 20px;\n  display: flex;\n  justify-content: space-between;\n  opacity: 0.5;\n  transition: opacity 0.3s ease;\n}\n\n.moles-container.game-active {\n  opacity: 1;\n}\n</style>"]}]}